{
    "name": "vscodeilviewer",
    "displayName": "IL Viewer",
    "description": "An IL Viewer for Visual Studio Code",
    "version": "0.0.1",
    "author": "Joseph Woodward",
    "license": "See licence in license file",
    "publisher": "josephwoodward",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages",
        "Other"
    ],
    "repository": "https://github.com/JosephWoodward/VSCodeILViewer",
    "activationEvents": [
        "onLanguage:csharp",
        "workspaceContains:project.json"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.showIlWindow",
            "title": "Inspect Intermediate Language"
        }],
        "menus": {
            "editor/context": [{
                "command": "extension.showIlWindow",
                "when": "editorTextFocus && editorLangId == csharp",
                "group": "navigation@1.31"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "request": "2.79.0",
        "@types/node": "^6.0.40",
        "omnisharp-client": "5.2.0",
        "@types/mocha": "^2.2.32"
    }
}