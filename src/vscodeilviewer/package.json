{
    "name": "vscodeilviewer",
    "displayName": "IL Viewer",
    "description": "An IL Viewer for Visual Studio Code",
    "version": "0.0.1",
    "author": "Joseph Woodward",
    "license": "See licence in license file",
    "publisher": "josephwoodward",
    "engines": {
        "vscode": "^1.5.0"
    },
    "icon": "logo.png",
    "galleryBanner": {
		"color": "#05644e",
		"theme": "dark"
	},
    "bugs": {
		"url": "https://github.com/JosephWoodward/VSCodeILViewer/issues",
		"email": "joseph.woodward@xeuse.com"
	},
    "categories": [
        "Languages",
        "Other"
    ],
    "repository": {
		"type": "git",
		"url": "https://github.com/JosephWoodward/VSCodeILViewer.git"
	},
    "homepage": "https://github.com/JosephWoodward/VSCodeILViewer/blob/master/README.md",
    "activationEvents": [
        "onLanguage:csharp",
        "workspaceContains:project.json"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.showIlWindow",
            "title": "Inspect IL"
        }],
        "menus": {
            "editor/context": [{
                "command": "extension.showIlWindow",
                "when": "editorTextFocus && editorLangId == csharp",
                "group": "navigation@1.31"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "request": "2.79.0",
        "find-parent-dir": "0.3.0"
    }
}