{
    "name": "vscodeilviewer",
    "displayName": "VsCodeIlViewer",
    "description": "IL Viewer for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "JosephWoodward",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:editor.showIlWindow"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "editor.showIlWindow",
            "title": "View IL"
        }],
        "menus": {
            "editor/context": [{
                "command": "editor.showIlWindow",
                "when": "editorHasReferenceProvider",
                "group": "navigation@1.31"
            }]
        },
        "keybindings": [{
            "command": "extension.showIlWindow",
            "key": "shift+ctrl+v",
            "mac": "shift+cmd+v",
            "when": "editorTextFocus && editorLangId == csharp"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}